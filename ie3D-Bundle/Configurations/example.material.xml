        <attribute type="std::string" getter="getTechniqueName" path="/material" attribute_name="technique_name"/>
        <attribute type="bool" getter="isDepthTest"  path="/material" attribute_name="is_depth_test"/>
        <attribute type="bool" getter="isDepthMask"  path="/material" attribute_name="is_depth_mask"/>
