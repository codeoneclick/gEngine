<model
    class_name="CConfigurationModel"
    base_class_name="IConfigurationGameObject"
    is_external="1">
    <attributes>
        <attribute type="std::string" is_convert_to_ogl_enum="0" getter="getMeshFilename" path="/model" attribute_name="mesh_filename"/>
        <attribute type="bool" is_convert_to_ogl_enum="0" getter="isBatching" path="/model" attribute_name="is_batching"/>
    </attributes>
    <relationships>
    	<relationship type="CConfigurationMaterial" getter="getMaterialsConfigurations" is_to_many="1" is_external="1" path="/model/materials" relationship_name="material"/>
    	<relationship type="CConfigurationAnimation" getter="getAnimationsConfigurations" is_to_many="1" is_external="0" path="/model/animations" relationship_name="animation"/>
    </relationships>
</model>